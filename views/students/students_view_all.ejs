<!DOCTYPE html>
<html>
<head>
    <title> Students - View All </title>
</head>
<body>

<h1>Students</h1>
<a href="/student/add">Add New A Student</a>
<table border="1">
    <tr>
        <th>ID</th>
        <th>Social Security Number</th>
        <th>Grade Level</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Dorm Name</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    <% for(var i = 0; i < students.length; i++) { %>
    <tr>
        <td><%= students[i].student_id %></td>
        <td><%= students[i].ssn %></td>
        <td><%= students[i].grade_level %></td>
        <td><%= students[i].first_name %></td>
        <td><%= students[i].last_name %></td>
        <td><%= students[i].dorm_name %></td>
        <td>
        <a href='/student/edit/?studet_id=<%= students[i].student_id %>'>
            Edit
        </a>
        </td>
        <td>
        <a href='/student/delete/?student_id=<%= students[i].student_id %>'>
            Delete
        </a>
        </td>
    </tr>
    <% } %>
</table>
<% if(locals.was_successful_delete) { %>
<div>
    <p>You Successfully Deleted A Student!</p>
</div>
<% } %>
</body>
</html>